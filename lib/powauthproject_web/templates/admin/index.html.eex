<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Role</th>


      <th></th>
    </tr>
  </thead>
  <tbody>

<%= for user <- @users do %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.role %></td>

      <td>

        <span><%= link "Edit", to: Routes.admin_path(@conn, :edit, user),   class: "button" %></span>

      </td>
    </tr>
<% end %>
  </tbody>
</table>
